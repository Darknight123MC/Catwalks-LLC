{
  "multipart": [
    {   
      "when": { "north_west": true },
      "apply": { "model": "catwalksinc:block/iron_rod" }
    },
    {   
      "when": { "north_east": true },
      "apply": { "model": "catwalksinc:block/iron_rod", "uvlock": true, "y": 90 }
    },
    {   
      "when": { "south_east": true },
      "apply": { "model": "catwalksinc:block/iron_rod", "uvlock": true, "y": 180 }
    },
    {   
      "when": { "south_west": true },
      "apply": { "model": "catwalksinc:block/iron_rod", "uvlock": true, "y": 270 }
    }
  ]
}