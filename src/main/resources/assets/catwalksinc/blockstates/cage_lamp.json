{
  "multipart": [
    {   
      "when": { "facing": "up" },
      "apply": { "model": "catwalksinc:block/cage_lamp" }
    },
    {   
      "when": { "facing": "down" },
      "apply": { "model": "catwalksinc:block/cage_lamp", "x": 180 }
    },
    {   
      "when": { "facing": "north" },
      "apply": { "model": "catwalksinc:block/cage_lamp", "x": 90 }
    },
    {   
      "when": { "facing": "south" },
      "apply": { "model": "catwalksinc:block/cage_lamp", "x": 90, "y": 180 }
    },
    {   
      "when": { "facing": "west" },
      "apply": { "model": "catwalksinc:block/cage_lamp", "x": 90, "y": 270 }
    },
    {   
      "when": { "facing": "east" },
      "apply": { "model": "catwalksinc:block/cage_lamp", "x": 90, "y": 90 }
    }
  ]
}