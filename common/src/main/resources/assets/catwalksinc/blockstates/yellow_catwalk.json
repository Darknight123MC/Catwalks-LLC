{
  "multipart": [
    {   
      "apply": { "model": "catwalksinc:block/yellow_catwalk_treadplate" }
    },
    {   
      "when": { "north": true },
      "apply": { "model": "catwalksinc:block/yellow_catwalk_fence" }
    },
    {   
      "when": { "east": true },
      "apply": { "model": "catwalksinc:block/yellow_catwalk_fence", "uvlock": true, "y": 90 }
    },
    {   
      "when": { "south": true },
      "apply": { "model": "catwalksinc:block/yellow_catwalk_fence", "uvlock": true, "y": 180 }
    },
    {   
      "when": { "west": true },
      "apply": { "model": "catwalksinc:block/yellow_catwalk_fence", "uvlock": true, "y": 270 }
    }
  ]
}